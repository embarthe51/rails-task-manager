<h1>Edit your task</h1>

<%= form_with model: @task do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :details %>
  <%= f.text_field :details %>
  <%= f.check_box (:completed) %>
  <%= f.label :completed %>
  <%= f.submit %>


<% end %>

<li><%= link_to 'Back to list', tasks_path %> </li>
